{
    "name": "Hello From Content Script",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["jquery-3.6.0.min.js", "content.js"]
        }
      ]
  }